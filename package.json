    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5"
  },
  "private": true,
  "prettier": {
    "trailingComma": "es5",
